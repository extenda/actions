{
  "name": "actions",
  "version": "0.0.1-local",
  "description": "Extenda Retail GitHub Actions",
  "private": true,
  "scripts": {
    "build": "node .scripts/build-modules.js",
    "build:docker": "run-script-os",
    "build:docker:win32": "docker pull node:16-alpine && docker run --rm -t -v \"%cd%\":/work -w /work node:16-alpine sh -c \"npm run build\"",
    "build:docker:default": "docker pull node:16-alpine && docker run --rm -t -v $(pwd):/work -w /work node:16-alpine sh -c 'npm ci && npm run build'",
    "install": "node .scripts/install-modules.js",
    "test": "jest --silent",
    "posttest": "node .scripts/create-sonar-properties.js",
    "lint:js": "run-script-os",
    "lint:js:win32": "docker run --rm -t -v \"%cd%\":/work -w /work node:16-alpine sh -c \"npm run lint:js\"",
    "lint:js:default": "eslint .scripts/*.js */src/*.js */test/*.js --no-ignore --fix"
  },
  "repository": "github:extenda/actions",
  "author": {
    "name": "Extenda Retail",
    "email": "info@extendaretail.com",
    "url": "https://extendaretail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/extenda/actions#readme",
  "devDependencies": {
    "@actions/exec": "^1.1.1",
    "chalk": "^4.1.2",
    "esbuild": "^0.15.14",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "fs-extra": "^10.1.0",
    "jest": "^29.1.2",
    "jest-sonar": "^0.2.12",
    "run-script-os": "^1.1.6"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "request": "^2.88.2"
  }
}
